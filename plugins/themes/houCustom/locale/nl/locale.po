msgid "plugins.themes.houCustom.name"
msgstr "Standaard stijl"

msgid "plugins.themes.houCustom.description"
msgstr "Deze stijl is de standaard weergave."

msgid "plugins.themes.houCustom.option.typography.label"
msgstr "Lettertype"

msgid "plugins.themes.houCustom.option.typography.description"
msgstr "Kies een combinatie van fonts voor het tijdschrift."

msgid "plugins.themes.houCustom.option.typography.notoSans"
msgstr ""
"Noto Sans: een zuiver digitale font, ontworpen door Google, met uitgebreide "
"dekking van talen."

msgid "plugins.themes.houCustom.option.typography.notoSerif"
msgstr "Noto Serif: een serif variant van Google's zuiver digitale font."

msgid "plugins.themes.houCustom.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: een combinatie van serif voor titels en sans-serif "
"voor lopende tekst."

msgid "plugins.themes.houCustom.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: een combinatie van sans-serif voor titels en serif "
"voor lopende tekst."

msgid "plugins.themes.houCustom.option.typography.lato"
msgstr "Lato: een populaire moderne sans-serif font."

msgid "plugins.themes.houCustom.option.typography.lora"
msgstr "Lora: een brede serif font die makkelijk leesbaar is online."

msgid "plugins.themes.houCustom.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: een combinatie van serif voor titels en sans-serif voor "
"lopende tekst."

msgid "plugins.themes.houCustom.option.colour.label"
msgstr "Kleur"

msgid "plugins.themes.houCustom.option.colour.description"
msgstr "Kies een kleur voor de header."

msgid "plugins.themes.houCustom.option.showDescriptionInJournalIndex.option"
msgstr "Toon de samenvatting van het tijdschrift op de startpagina."

msgid "plugins.themes.houCustom.option.useHomepageImageAsHeader.label"
msgstr "Achtergrondafbeelding header"

msgid "plugins.themes.houCustom.option.useHomepageImageAsHeader.description"
msgstr ""
"Als een startpagina-afbeelding is ge√ºpload, toon deze dan op de achtergrond "
"van de header in plaats van op de gebruikelijke positie op de startpagina."

msgid "plugins.themes.houCustom.option.useHomepageImageAsHeader.option"
msgstr "Toon de startpagina-afbeelding als achtergrond van de header."

msgid "plugins.themes.houCustom.option.displayStats.label"
msgstr "Gebruiksstatistieken weergaveopties"

msgid "plugins.themes.houCustom.option.displayStats.none"
msgstr ""
"Geef geen grafiek met gebruiksstatistieken voor inzendingen weer voor de "
"lezer."

msgid "plugins.themes.houCustom.option.displayStats.bar"
msgstr "Gebruik een staafdiagram voor het weergeven van gebruiksstatistieken."

msgid "plugins.themes.houCustom.option.displayStats.line"
msgstr "Gebruik een lijngrafiek voor het weergeven van gebruiksstatistieken."

msgid "plugins.themes.houCustom.displayStats.monthInitials"
msgstr "Jan Feb Mar Apr Mei Jun Jul Aug Sep Okt Nov Dec"

